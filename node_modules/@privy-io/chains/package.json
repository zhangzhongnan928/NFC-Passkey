{
  "name": "@privy-io/chains",
  "version": "0.0.1",
  "description": "Network configurations for Privy SDKs",
  "keywords": [],
  "license": "Apache-2.0",
  "author": "privy.io",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/dts/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/dts/index.d.mts",
        "default": "./dist/esm/index.mjs"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/dts/index.d.ts",
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rollup --config rollup.types.mjs && rollup --config rollup.build.mjs",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist .turbo",
    "dev": "concurrently --prefix=none \"rollup --config rollup.types.mjs --watch\" \"rollup --config rollup.dev.mjs --watch\"",
    "format": "eslint src --fix",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browserslist": [
    "defaults",
    "not op_mini all"
  ],
  "dependencies": {},
  "devDependencies": {
    "@privy-io/build-config": "*",
    "@privy-io/eslint-config-custom": "*",
    "@privy-io/tsconfig": "*",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@swc/core": "^1.10.0",
    "@swc/plugin-styled-components": "^6.0.1",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "glob": "^10.3.12",
    "jest": "^29.7.0",
    "@swc/jest": "0.2.37",
    "rollup": "^4.28.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "~5.5.3"
  },
  "peerDependencies": {},
  "peerDependenciesMeta": {},
  "publishConfig": {
    "access": "public"
  }
}
